<?xml version="1.0" encoding="UTF-8" ?>
<Configuration satus="WARN">
    <Appenders>
        <console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="%highlight{%d{dd/MM/yyyy - HH:mm:ss} %level %msg%n}{INFO=green} " disableAnsi="false"/>
        </console>
    </Appenders>
    <Loggers>
        <Root Level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>